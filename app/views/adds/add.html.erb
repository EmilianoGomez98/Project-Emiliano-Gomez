<h3>You are now Adding new data</h3>
<h5>Please enter the information below</h5>
<p id="error"><%= flash[:error] %></p>

<%= form_tag '/adds/create' do %>
<p>
  <%= label_tag(:key, "Key: ") %><br>
   <%= text_field_tag :key %>
 </p>
   <p>
     <%= label_tag(:flag, "Flag: ") %><br>
      <%= text_field_tag :flag %>
    </p>
 <p>
   <%= label_tag(:timeToLive, "Time to Live: ") %><br>
    <%= text_field_tag :timeToLive %>
    </p>
    <p>
      <%= label_tag(:bytes, "Bytes: ") %><br>
       <%= text_field_tag :bytes %>
     </p>
   <p>
     <%= label_tag(:value, "Value: ") %><br>
     <%= text_field_tag :value %>
    </p>
<p>
  <%= submit_tag("Submit") %>
</p>
<p><%=link_to 'Back',root_path %></p>

<% end %>
